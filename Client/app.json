{
  "expo": {
    "name": "CozySpace",
    "slug": "AnxiEase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": [
      "cozyspace"
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.finalproject.cozyspace",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "processing",
          "bluetooth-central"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "foregroundService": {
        "type": ["dataSync"]
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.cozyspace_finalproject.CozySpace"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for emergency services.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for emergency services.",
          "isIosBackgroundLocationEnabled": false
        }
      ],
      [
        "expo-background-fetch",
        {
          "startOnBoot": true,
          "minimumInterval": 900
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a85fc8f4-3269-42ba-93a7-6f509bdabf9f"
      }
    },
    "owner": "cozyspace_finalproject",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a85fc8f4-3269-42ba-93a7-6f509bdabf9f"
    }
  }
}
